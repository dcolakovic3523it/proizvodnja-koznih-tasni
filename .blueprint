    Narudzbina: { user_id: 'id foreign:User', ukupna_cena: 'decimal:8,2', status: 'string:50' }
    StavkaNarudzbine: { proizvod_id: 'id foreign:Proizvod', narudzbina_id: 'id foreign:Narudzbina', kolicina: integer, cena: 'decimal:8,2' }
